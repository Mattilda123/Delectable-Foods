    {
      "id": "bre_001",
      "name": "Century Egg Pork Porridge (Pi Dan Shou Rou Zhou)",
      "difficulty": "Easy",
      "prep_time": "15 minutes",
      "cook_time": "60 minutes",
      "servings": 4.0,
      "description": "A classic Chinese comfort food in Singapore, featuring silky rice porridge simmered with marinated pork and century egg, topped with spring onion, ginger, and fried shallots. Popular at hawker stalls and for breakfast.",
      "ingredients": {
        "main": {
          "rice": "1 cup, rinsed",
          "water or stock": "8 cups",
          "pork loin or shoulder, thinly sliced": "200g",
          "century eggs": "2, peeled and diced",
          "ginger": "4 slices, julienned",
          "light soy sauce": "1 tbsp",
          "Shaoxing wine": "1 tbsp",
          "white pepper": "to taste",
          "salt": "to taste",
          "sesame oil": "1 tsp",
          "cornstarch": "1 tsp (for pork marinade)"
        },
        "for serving": {
          "spring onion": "2, chopped",
          "fried shallots": "2 tbsp",
          "youtiao (fried dough sticks)": "sliced, optional"
        }
      },
      "steps": [
        "Marinate pork with soy sauce, Shaoxing wine, white pepper, sesame oil, and cornstarch. Set aside.",
        "Bring rice and water/stock to a boil in a large pot. Reduce heat and simmer, stirring occasionally, until porridge is creamy (about 45 minutes).",
        "Add marinated pork and half the century egg. Simmer for 10 minutes until pork is cooked and porridge is silky.",
        "Season with salt and pepper. Stir in remaining century egg. Serve hot, topped with ginger, spring onion, fried shallots, and youtiao if desired."
      ],
      "tips": [
        "Stir porridge frequently to prevent sticking.",
        "Add more water for a thinner consistency.",
        "Can add preserved egg white for extra flavor."
      ],
