    {
      "id": "bre_002",
      "name": "Teochew Pumpkin Porridge (Teochew Nam Kua Zhou)",
      "difficulty": "Easy",
      "prep_time": "10 minutes",
      "cook_time": "40 minutes",
      "servings": 4.0,
      "description": "A classic Teochew/Chinese porridge in Singapore, featuring soft rice grains simmered with sweet pumpkin chunks. Served with simple side dishes, it's a comforting, home-style meal.",
      "ingredients": {
        "main": {
          "rice": "1 cup, rinsed",
          "water": "8 cups",
          "pumpkin, peeled and cubed": "300g",
          "salt": "to taste",
          "white pepper": "to taste"
        },
        "for serving": {
          "spring onion": "chopped, optional",
          "side dishes": "such as salted egg, pickled vegetables, or braised peanuts"
        }
      },
      "steps": [
        "Bring rice and water to a boil in a large pot. Reduce heat and simmer, stirring occasionally, until rice grains are soft but still whole (about 30 minutes).",
        "Add pumpkin cubes and simmer for another 10-15 minutes until pumpkin is tender but not mushy.",
        "Season with salt and white pepper. Serve hot, garnished with spring onion and accompanied by classic Teochew side dishes."
      ],
      "tips": [
        "Use Japanese or local pumpkin for best sweetness and texture.",
        "Teochew porridge is typically watery with distinct rice grains, not creamy.",
        "Serve with a variety of simple side dishes for a complete meal."
      ],
