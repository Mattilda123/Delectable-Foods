    {
      "id": "eur_001",
      "name": "Feng (Eurasian Christmas Stew)",
      "difficulty": "Hard",
      "prep_time": "1 hour",
      "cook_time": "2 hours",
      "servings": 8.0,
      "description": "Rich, spicy stew of pork, liver, and spices, a Eurasian Christmas classic.",
      "ingredients": {
        "meat": {
          "pork belly": "500g, cubed",
          "pork liver": "200g, cubed"
        },
        "spice paste": {
          "shallots": "8",
          "garlic": "4 cloves",
          "candlenuts": "6",
          "dried chilies": "8, soaked",
          "galangal": "2cm, sliced",
          "lemongrass": "2 stalks, sliced",
          "coriander powder": "1 tbsp",
          "cumin powder": "1 tsp",
          "cloves": "4",
          "cinnamon stick": "1"
        },
        "other": {
          "vinegar": "2 tbsp",
          "sugar": "2 tbsp",
          "salt": "to taste",
          "water": "1L"
        }
      },
      "steps": [
        "Blend spice paste ingredients until smooth. Fry in oil until fragrant.",
        "Add pork and liver, brown lightly. Add vinegar, sugar, salt, and water. Simmer 2 hours until thick.",
        "Serve with steamed rice or bread."
      ],
      "tips": [
        "Let stew rest overnight for deeper flavor.",
        "Adjust vinegar for desired tanginess."
      ],
