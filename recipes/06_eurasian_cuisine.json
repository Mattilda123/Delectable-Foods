    {
      "id": "eur_005",
      "name": "Hakka Fried Pork (Zha Yuk)",
      "difficulty": "Medium",
      "prep_time": "20 minutes (plus marinating)",
      "cook_time": "30 minutes",
      "servings": 4.0,
      "description": "A Hakka and Eurasian favorite: pork belly marinated with red fermented bean curd, five spice, and rice wine, then deep-fried until crispy. Served with rice or as part of festive meals.",
      "ingredients": {
        "Main": {
          "pork belly": "500g, sliced",
          "red fermented bean curd (nam yue)": "2 cubes, mashed",
          "five spice powder": "1 tsp",
          "Shaoxing wine": "1 tbsp",
          "soy sauce": "1 tbsp",
          "sugar": "1 tsp",
          "white pepper": "1/2 tsp",
          "cornstarch": "2 tbsp",
          "oil": "for deep frying"
        }
      },
      "preparation": [
        "Mix pork with all marinade ingredients except cornstarch. Marinate at least 2 hours or overnight.",
        "Toss marinated pork with cornstarch. Deep-fry in hot oil until golden and crisp. Drain on paper towels.",
        "Serve hot with rice or as part of a festive spread."
      ],
      "tips": [
