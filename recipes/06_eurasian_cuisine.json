    {
      "id": "eur_002",
      "name": "Sugee Cake",
      "difficulty": "Medium",
      "prep_time": "1 hour",
      "cook_time": "1 hour",
      "servings": 12.0,
      "description": "Rich, buttery semolina cake with ground almonds, a Eurasian festive treat.",
      "ingredients": {
        "cake": {
          "semolina (sugee) flour": "200g",
          "ground almonds": "100g",
          "plain flour": "50g",
          "baking powder": "1 tsp",
          "butter": "250g, softened",
          "sugar": "200g",
          "eggs": "6",
          "brandy": "2 tbsp (optional)",
          "vanilla extract": "1 tsp"
        }
      },
      "steps": [
        "Cream butter and sugar until light. Add eggs one at a time, then brandy and vanilla.",
        "Fold in semolina, almonds, flour, and baking powder. Pour into lined tin.",
        "Bake at 170°C for 50–60 minutes until golden and set. Cool before slicing."
      ],
      "tips": [
        "Let batter rest 1 hour before baking for best texture.",
        "Add orange zest for extra aroma."
