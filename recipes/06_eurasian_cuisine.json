    {
      "id": "eur_003",
      "name": "Shepherd's Pie (Eurasian Style)",
      "difficulty": "Medium",
      "prep_time": "30 minutes",
      "cook_time": "1 hour 30 minutes",
      "servings": 6.0,
      "description": "A comforting Eurasian-style Shepherd's Pie with a rich lamb or beef filling, carrots, peas, and onions, topped with creamy mashed potatoes and baked until golden. Adapted from classic British and Eurasian home recipes.",
      "ingredients": {
        "main": {
          "ground lamb or beef": "1 kg (2.2 lbs)",
          "onion": "1 large, diced",
          "carrots": "3 medium, diced",
          "celery": "2 ribs, diced",
          "garlic": "4 cloves, minced",
          "frozen peas": "200g (1 cup)",
          "tomato paste": "2 tbsp",
          "red wine": "120ml (1/2 cup)",
          "beef or chicken stock": "360ml (1.5 cups)",
          "Worcestershire sauce": "2 tbsp",
          "thyme": "2 sprigs, fresh (or 1 tsp dried)",
          "bay leaf": "1",
          "olive oil": "2 tbsp",
          "salt and pepper": "to taste"
        },
        "mashed potatoes": {
          "russet potatoes": "1.5 kg (3.3 lbs)",
          "unsalted butter": "85g (6 tbsp)",
          "heavy cream": "360ml (1.5 cups)",
          "parmesan cheese (optional)": "1/2 cup, grated",
          "salt": "to taste"
        }
      },
      "instructions": [
        "Peel and cube potatoes. Boil in salted water until tender, drain, and mash with butter, cream, and salt. Stir in parmesan if using. Set aside.",
        "Heat olive oil in a large pan. Sauté onion, carrots, celery, and garlic until soft.",
        "Add ground lamb or beef and cook until browned. Stir in tomato paste and cook for 1 minute.",
        "Add red wine and cook until mostly evaporated. Add stock, Worcestershire, thyme, bay leaf, salt, and pepper. Simmer for 20 minutes until thickened. Remove thyme and bay leaf.",
        "Stir in peas and cook for 2 minutes. Transfer meat mixture to a baking dish.",
        "Spread mashed potatoes evenly over the meat. Use a fork to create ridges. Sprinkle with parmesan if desired.",
        "Bake at 220°C (425°F) for 20-25 minutes until golden. Let rest 10 minutes before serving."
      ],
